  },
  "main": "./sources/index.ts",
  "devDependencies": {
    "@types/eslint": "^6.1.1",
    "@types/estree": "^0.0.39",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^3.1.2"
  },
  "peerDependencies": {
    "eslint": "^3.18.0 || ^4.0.0 || ^5.0.0 || ^6.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "scripts": {
    "test:eslint-plugin-pnp": "run test:unit packages/yarnpkg-eslint-plugin-pnp",
    "test:watch:eslint-plugin-pnp": "run test:unit --watch packages/yarnpkg-eslint-plugin-pnp",
    "postpack": "rm -rf lib",
    "prepack": "mkdir -p lib && rsync -a --include '*.d.ts' --exclude '*.ts' sources/ lib/ && run build:compile packages/yarnpkg-eslint-plugin-pnp",
    "release": "yarn npm publish"
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts"
  },
  "files": [
    "/lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/yarnpkg/berry.git"
  }
